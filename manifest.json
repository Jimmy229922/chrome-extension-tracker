{
  "manifest_version": 3,
  "name": "Account Copy Tracker (Side Panel)",
  "version": "3.7",
  "description": "إكستنشن لتتبع الحسابات والـ IPs المُنسوخة، مع إشعارات VIP وإدارة ملاحظات. آمن ومفيد للمستخدمين.",
  "author": "Jimmy229922",
  "homepage_url": "https://github.com/Jimmy229922/chrome-extension-tracker",
  "permissions": [
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "sidePanel",
    "offscreen",
    "notifications",
    "tabs",
    "windows",
    "downloads"
  ],
  "host_permissions": [
    "https://ipwhois.app/"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {},
  "options_page": "options.html",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [ "images/magnifying_glass.png" ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["csp-guard.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}